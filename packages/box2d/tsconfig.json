{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "inlineSourceMap": true,
    "esModuleInterop": true,
    "noEmit": true,
    "rootDir": "./src",
    "baseUrl": "."
  },
  "include": ["./src/**/*.ts"]
}
